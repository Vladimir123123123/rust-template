using System.Runtime.InteropServices;

[StructLayout(LayoutKind.Explicit)]
public struct RGBQuad
{
	[FieldOffset(0)]
	public byte rgbBlue;

	[FieldOffset(1)]
	public byte rgbGreen;

	[FieldOffset(2)]
	public byte rgbRed;

	[FieldOffset(3)]
	public byte rgbReserved;

	[FieldOffset(0)]
	public uint uintValue;
}
