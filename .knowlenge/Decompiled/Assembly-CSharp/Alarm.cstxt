using System;
using ProtoBuf;

public struct Alarm
{
	public TimeSpan time;

	public bool active;

	public Alarm(TimeSpan time, bool active)
	{
		this.time = time;
		this.active = active;
	}

	public Alarm(DigitalClockAlarm alarm)
	{
		time = DigitalClockEx.ToTimeSpan(alarm.time);
		active = alarm.active;
	}
}
