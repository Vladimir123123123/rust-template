using System;
using UnityEngine;

[Serializable]
public struct CapsuleColliderInfo
{
	public float height;

	public float radius;

	public Vector3 center;

	public CapsuleColliderInfo(float height, float radius, Vector3 center)
	{
		this.height = height;
		this.radius = radius;
		this.center = center;
	}
}
