using System;

[Serializable]
public class EngineCycle
{
	public int RPM;

	public int startSample;

	public int endSample;

	public float period;

	public int id;

	public EngineCycle(int RPM, int startSample, int endSample, float period, int id)
	{
		this.RPM = RPM;
		this.startSample = startSample;
		this.endSample = endSample;
		this.period = period;
		this.id = id;
	}
}
