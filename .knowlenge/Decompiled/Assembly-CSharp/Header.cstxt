using System.IO;
using Network;
using ProtoBuf;

public class Header : DemoHeader, IDemoHeader
{
	long IDemoHeader.Length
	{
		get
		{
			return length;
		}
		set
		{
			length = value;
		}
	}

	public void Write(BinaryWriter writer)
	{
		byte[] array = ToProtoBytes();
		writer.Write("RUST DEMO FORMAT");
		writer.Write(array.Length);
		writer.Write(array);
		writer.Write('\0');
	}
}
