private class LegHelper
{
	public bool WasAttaching;

	public bool DetachTrigger;

	public float FullyAttachedAt = -1f;

	public LegsAnimator.Leg legRef { get; private set; }

	public LegHelper(LegsAnimator.Leg leg)
	{
		legRef = leg;
		WasAttaching = false;
		DetachTrigger = false;
		FullyAttachedAt = -1f;
	}
}
