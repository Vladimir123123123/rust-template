using System;

[Serializable]
public struct Skin
{
	public int id;

	public int itemid;

	public string name;

	public bool isSkin;

	private SteamInventoryItem _invItem;

	public SteamInventoryItem invItem
	{
		get
		{
			if (_invItem == null && !string.IsNullOrEmpty(name))
			{
				_invItem = FileSystem.Load<SteamInventoryItem>(name);
			}
			return _invItem;
		}
	}
}
