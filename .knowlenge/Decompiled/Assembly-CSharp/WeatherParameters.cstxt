using System;
using UnityEngine;

[Serializable]
public class WeatherParameters
{
	[Range(0f, 1f)]
	public float ClearChance = 1f;

	[Range(0f, 1f)]
	public float DustChance;

	[Range(0f, 1f)]
	public float FogChance;

	[Range(0f, 1f)]
	public float OvercastChance;

	[Range(0f, 1f)]
	public float StormChance;

	[Range(0f, 1f)]
	public float RainChance;
}
