using System;
using Unity.Mathematics;

[Serializable]
internal struct RotationConstraint
{
	public bool3 Constraints;

	public float2 X;

	public float2 Y;

	public float2 Z;

	public RotationConstraint(bool3 constraints, float2 x, float2 y, float2 z)
	{
		Constraints = constraints;
		X = x;
		Y = y;
		Z = z;
	}
}
