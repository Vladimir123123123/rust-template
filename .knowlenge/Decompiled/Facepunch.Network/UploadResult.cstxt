using System;

private class UploadResult
{
	public long Bytes;

	public TimeSpan TimeTaken;

	public int Concurrency;

	public long BandwidthPerSecondPerThread;

	public UploadResult(long bytes, TimeSpan time, int concurrency)
	{
		Bytes = bytes;
		TimeTaken = time;
		Concurrency = concurrency;
	}
}
