		}
		if (instance.hitPositionLocal != previous.hitPositionLocal)
		{
			stream.WriteByte(50);
			obj.SetLength(0L);
			Vector3Serialized.SerializeDelta(obj, instance.hitPositionLocal, previous.hitPositionLocal);
			uint num4 = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num4);
			stream.Write(obj.GetBuffer(), 0, (int)num4);
		}
		if (instance.hitNormalWorld != previous.hitNormalWorld)
		{
			stream.WriteByte(58);
			obj.SetLength(0L);
			Vector3Serialized.SerializeDelta(obj, instance.hitNormalWorld, previous.hitNormalWorld);
			uint num5 = (uint)obj.Length;
