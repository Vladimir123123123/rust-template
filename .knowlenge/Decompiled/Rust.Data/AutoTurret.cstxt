	public static void SerializeDelta(Stream stream, AutoTurret instance, AutoTurret previous)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		if (instance.aimPos != previous.aimPos)
		{
			stream.WriteByte(10);
			obj.SetLength(0L);
			Vector3Serialized.SerializeDelta(obj, instance.aimPos, previous.aimPos);
			uint num = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num);
			stream.Write(obj.GetBuffer(), 0, (int)num);
		}
		if (instance.aimDir != previous.aimDir)
		{
			stream.WriteByte(18);
			obj.SetLength(0L);
			Vector3Serialized.SerializeDelta(obj, instance.aimDir, previous.aimDir);
			uint num2 = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num2);
			stream.Write(obj.GetBuffer(), 0, (int)num2);
		}
		if (instance.targetID != previous.targetID)
		{
			stream.WriteByte(24);
			ProtocolParser.WriteUInt32(stream, instance.targetID);
		}
		if (instance.users != null)
		{
			for (int i = 0; i < instance.users.Count; i++)
			{
				PlayerNameID playerNameID = instance.users[i];
				stream.WriteByte(34);
				obj.SetLength(0L);
				PlayerNameID.SerializeDelta(obj, playerNameID, playerNameID);
				uint num3 = (uint)obj.Length;
				ProtocolParser.WriteUInt32(stream, num3);
				stream.Write(obj.GetBuffer(), 0, (int)num3);
			}
		}
		Pool.FreeUnmanaged(ref obj);
	}

