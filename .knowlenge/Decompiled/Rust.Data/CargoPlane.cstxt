	public static void SerializeDelta(Stream stream, CargoPlane instance, CargoPlane previous)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		if (instance.startPos != previous.startPos)
		{
			stream.WriteByte(10);
			obj.SetLength(0L);
			Vector3Serialized.SerializeDelta(obj, instance.startPos, previous.startPos);
			uint num = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num);
			stream.Write(obj.GetBuffer(), 0, (int)num);
		}
		if (instance.endPos != previous.endPos)
		{
			stream.WriteByte(18);
			obj.SetLength(0L);
			Vector3Serialized.SerializeDelta(obj, instance.endPos, previous.endPos);
			uint num2 = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num2);
			stream.Write(obj.GetBuffer(), 0, (int)num2);
		}
		if (instance.secondsToTake != previous.secondsToTake)
		{
			stream.WriteByte(29);
			ProtocolParser.WriteSingle(stream, instance.secondsToTake);
		}
		if (instance.secondsTaken != previous.secondsTaken)
		{
			stream.WriteByte(37);
			ProtocolParser.WriteSingle(stream, instance.secondsTaken);
		}
		stream.WriteByte(40);
		ProtocolParser.WriteBool(stream, instance.dropped);
		if (instance.dropPosition != previous.dropPosition)
		{
			stream.WriteByte(50);
			obj.SetLength(0L);
			Vector3Serialized.SerializeDelta(obj, instance.dropPosition, previous.dropPosition);
			uint num3 = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num3);
			stream.Write(obj.GetBuffer(), 0, (int)num3);
		}
		Pool.FreeUnmanaged(ref obj);
	}

	public static void Serialize(Stream stream, CargoPlane instance)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		if (instance.startPos != default(Vector3))
		{
			stream.WriteByte(10);
			obj.SetLength(0L);
			Vector3Serialized.Serialize(obj, instance.startPos);
			uint num = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num);
			stream.Write(obj.GetBuffer(), 0, (int)num);
		}
		if (instance.endPos != default(Vector3))
