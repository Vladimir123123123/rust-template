		if (instance.data != null)
		{
			stream.WriteByte(18);
			obj.SetLength(0L);
			FarmableAnimal.Serialize(obj, instance.data);
			uint num = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num);
			stream.Write(obj.GetBuffer(), 0, (int)num);
