			stream.Write(obj.GetBuffer(), 0, (int)num3);
		}
		if (instance.spotlightVec != previous.spotlightVec)
		{
			stream.WriteByte(34);
			obj.SetLength(0L);
			Vector3Serialized.SerializeDelta(obj, instance.spotlightVec, previous.spotlightVec);
			uint num4 = (uint)obj.Length;
