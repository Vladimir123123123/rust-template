			for (int i = 0; i < fogImages.Count; i++)
			{
				uint item = fogImages[i];
				instance.fogImages.Add(item);
			}
		}
		else
		{
			instance.fogImages = null;
		}
		if (paintImages != null)
		{
			instance.paintImages = Pool.Get<List<uint>>();
