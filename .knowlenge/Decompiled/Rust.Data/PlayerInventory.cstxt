	public static void SerializeDelta(Stream stream, PlayerInventory instance, PlayerInventory previous)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		if (instance.invMain != null)
		{
			stream.WriteByte(10);
			obj.SetLength(0L);
			ItemContainer.SerializeDelta(obj, instance.invMain, previous.invMain);
			uint num = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num);
			stream.Write(obj.GetBuffer(), 0, (int)num);
		}
		if (instance.invBelt != null)
		{
			stream.WriteByte(18);
			obj.SetLength(0L);
			ItemContainer.SerializeDelta(obj, instance.invBelt, previous.invBelt);
