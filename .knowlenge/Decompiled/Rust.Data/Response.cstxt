	public static void SerializeDelta(Stream stream, Response instance, Response previous)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		stream.WriteByte(10);
		obj.SetLength(0L);
		UuidSerialized.SerializeDelta(obj, instance.id, previous.id);
		uint num = (uint)obj.Length;
		ProtocolParser.WriteUInt32(stream, num);
		stream.Write(obj.GetBuffer(), 0, (int)num);
		if (instance.status == null)
		{
			throw new ArgumentNullException("status", "Required by proto specification.");
		}
		stream.WriteByte(18);
		obj.SetLength(0L);
		Status.SerializeDelta(obj, instance.status, previous.status);
		uint num2 = (uint)obj.Length;
		ProtocolParser.WriteUInt32(stream, num2);
		stream.Write(obj.GetBuffer(), 0, (int)num2);
		if (instance.ping != null)
		{
			stream.WriteByte(26);
			obj.SetLength(0L);
			PingResponse.SerializeDelta(obj, instance.ping, previous.ping);
			uint num3 = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num3);
			stream.Write(obj.GetBuffer(), 0, (int)num3);
		}
		if (instance.spawnOptions != null)
		{
			stream.WriteByte(34);
			obj.SetLength(0L);
			SpawnOptionsResponse.SerializeDelta(obj, instance.spawnOptions, previous.spawnOptions);
			uint num4 = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num4);
			stream.Write(obj.GetBuffer(), 0, (int)num4);
		}
		if (instance.ferryStatus != null)
		{
			stream.WriteByte(42);
			obj.SetLength(0L);
			FerryStatusResponse.SerializeDelta(obj, instance.ferryStatus, previous.ferryStatus);
			uint num5 = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num5);
			stream.Write(obj.GetBuffer(), 0, (int)num5);
		}
		Pool.FreeUnmanaged(ref obj);
	}

