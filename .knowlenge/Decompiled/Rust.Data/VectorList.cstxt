				Vector3 vector = instance.vectorPoints[i];
				stream.WriteByte(10);
				obj.SetLength(0L);
				Vector3Serialized.SerializeDelta(obj, vector, vector);
				uint num = (uint)obj.Length;
				ProtocolParser.WriteUInt32(stream, num);
				stream.Write(obj.GetBuffer(), 0, (int)num);
