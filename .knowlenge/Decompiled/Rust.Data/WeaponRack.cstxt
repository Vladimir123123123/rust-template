	public static void SerializeDelta(Stream stream, WeaponRack instance, WeaponRack previous)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		if (instance.items != null)
		{
			for (int i = 0; i < instance.items.Count; i++)
			{
				WeaponRackItem weaponRackItem = instance.items[i];
				stream.WriteByte(10);
				obj.SetLength(0L);
				WeaponRackItem.SerializeDelta(obj, weaponRackItem, weaponRackItem);
				uint num = (uint)obj.Length;
				ProtocolParser.WriteUInt32(stream, num);
				stream.Write(obj.GetBuffer(), 0, (int)num);
			}
		}
		Pool.FreeUnmanaged(ref obj);
	}

