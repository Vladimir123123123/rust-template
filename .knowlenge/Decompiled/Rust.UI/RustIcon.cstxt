using Rust.UI;
using TMPro;
using UnityEngine;

[AddComponentMenu("Rust/UI/RustIcon")]
public class RustIcon : TextMeshProUGUI
{
	[SerializeField]
	private Icons _icon;

	public Icons Icon
	{
		get
		{
			return _icon;
		}
		set
		{
			_icon = value;
			base.text = $"{(char)value}";
		}
	}

	public void SetIcon(Icons newIcon)
	{
		Icon = newIcon;
	}
}
